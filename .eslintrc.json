{
    "env": {
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "arrow-parens":"off",
        "comma-dangle":"off",
        "linebreak-style":"off",
        "spaced-comment":"off",
        "object-curly-newline":"off"
    }
}